application/pdf;evince %s 2>&1 >/dev/null &;test=[ -n "${DISPLAY}" ];needsterminal
application/x-pkcs7-signature;openssl pkcs7 -in %s -inform der -noout -print_certs -text | less; needsterminal
image/*;firefox %s;test=[ -n "${DISPLAY}" ];needsterminal
text/calendar;gcalcli --calendar Work -v import %s;nametemplate=%s.ics
text/html;firefox %s;test=[ -n "${DISPLAY}" ];needsterminal
text/html;w3m -I %{charset} -O UTF-8 -dump %s;nametemplate=%s.html;copiousoutput
